{
  "kind": "collectionType",
  "collectionName": "restaurants",
  "info": {
    "name": "Restaurant",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "image": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "province": {
      "type": "string",
      "required": true,
      "default": "none"
    },
    "dishes": {
      "via": "restaurant",
      "collection": "dish"
    }
  }
}
